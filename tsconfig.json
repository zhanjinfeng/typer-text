{
    "compilerOptions": {
        "target": "es5",
        "module": "esnext",
        "moduleResolution": "node",
        "declaration": true,
        "outDir": "./dist/lib",
        "strict": true,
        "lib": ["esnext", "dom"],  // 导入库类型定义
        "allowJs": true,
        "resolveJsonModule": true
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules",
        "test/**",
        "example",
    ],
}